{
    "name": "chivincent/youku-php-sdk",
    "description": "Unofficial PHP SDK for youku.com",
    "type": "library",
    "license": "MIT",
    "keywords": [
	    "PHP", "PHP SDK", "youku", "youku.com"
    ],
    "authors": [
        {
            "name": "Vincent Chi",
            "email": "song374561@chivincent.net"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "ext-json": "*",
        "ext-curl": "*",
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.3",
        "aliyuncs/oss-sdk-php": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Chivincent\\Youku\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    }
}
